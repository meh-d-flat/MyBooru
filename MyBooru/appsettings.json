{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "Store": {
    "ConnectionString": "Data Source=db.sqlite3",
    "MainTable": "Media"
  },
  "ConnectionStrings": {
    "DefaultConnection": "Data Source=db.sqlite3"
  },
  "FilePath": "Files",
  "Limiter": {
    "CleanupIntervalMs": 2000,
    "RequestsNumber": 20
  },
  "SQLiteDBPath": "db.sqlite3",
  "FFMpegExecPath": [ "ffmpeg/bin/ffmpeg.exe", "ffmpeg/bin/ffmpeg" ],
  "FFProbeExecPath": [ "ffmpeg/bin/ffprobe.exe", "ffmpeg/bin/ffprobe" ],
  "ApiConsumerAddressPort": [ "https://192.168.0.107:44353", "https://localhost:44353" ],
  "ExternalFileUploadAllowed": true
}
